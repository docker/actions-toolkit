{
  "group": {
    "default": {
      "targets": [
        "image"
      ]
    }
  },
  "target": {
    "image": {
      "context": ".",
      "dockerfile": "Dockerfile",
      "args": {
        "BUILDKIT_CONTEXT_KEEP_GIT_DIR": "1",
        "GO_VERSION": "1.20"
      },
      "tags": [
        "docker/buildx-bin:local"
      ],
      "target": "binaries",
      "platforms": [
        "linux/amd64"
      ],
      "output": [
        "type=docker"
      ]
    }
  }
}
